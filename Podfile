  pod 'Moya' #网络请求框架
  pod 'SnapKit' #类似于OC中的masonry的自动布局框架
  pod 'Kingfisher' #SDWebImage swift 版
  pod 'IQKeyboardManagerSwift' #键盘管理
  pod 'SwiftyJSON' #json转模型
  
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxGesture'
  
  pod 'SwifterSwift' # 常用拓展
  pod 'ESTabBarController-swift' # TabBar
  
  pod 'EmptyDataSet-Swift' #  列表空数据图文展示
  pod 'SwiftyFitsize' # 字体适配
  pod 'SKPhotoBrowser' # 图片预览
  pod 'JXSegmentedView' # 分类滚动
  
  pod "ESPullToRefresh" # 上下拉
  pod 'MJRefresh' #3.4.3

  # Pods for SwiftActualCombat

  target 'SwiftActualCombatTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SwiftActualCombatUITests' do
    # Pods for testing
  end

end
