  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  pod 'VODUpload'
  pod 'AliPlayerSDK_iOS'
  pod 'PolyvVodSDK' #保利威视频
  pod 'PLVVodDanmu'
  pod 'PLVSubtitle'
  pod 'PLVMasonry'
  pod 'YYWebImage'
  pod 'FDStackView'
